{
    "name": "devtemple/laravel-lang",
    "description": "Package for managing JSON language files with pretty management panel.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Kuriata",
            "email": "adriandj83@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "tightenco/ziggy": "dev"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Devtemple\\LaravelLang\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Devtemple\\LaravelLang\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devtemple\\LaravelLang\\LaravelLangServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
